{
    "name": "philiplb/crudlex",
    "type": "library",
    "description": "An easy to use CRUD generator for Silex and Symfony 4.",
    "keywords": ["symfony", "silex", "crud", "admin generator"],
    "homepage": "https://github.com/philiplb/CRUDlex",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Lehmann-BÃ¶hm",
            "email": "philip@philiplb.de"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/twig-bridge": "^4.1",
        "doctrine/dbal": "^2.5",
        "symfony/http-foundation": "^4.1",
        "symfony/routing": "^4.1",
        "symfony/yaml": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/intl": "^4.1",
        "philiplb/valdi": "^1.0",
        "romaricdrigon/metayaml": "dev-master#1481b99679b29cbb50c88f3e10e4951ac64b8ad3",
        "riimu/kit-phpencoder": "^2.1",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "symfony/browser-kit": "^4.1",
        "symfony/css-selector": "^4.1",
        "sphpdox/sphpdox": "dev-master#bc149dd0a0e3ec1f3e0d24d752552f54bfa17a53",
        "eloquent/phony": "^3.0",
        "eloquent/phony-phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": { "CRUDlex\\": "src/CRUDlex" }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.15.x-dev"
        }
    }
}
